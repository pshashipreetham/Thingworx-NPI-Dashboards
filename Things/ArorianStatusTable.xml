                                        var parsed = Things["Temp_WindchillIntegrationThing"].processString();
                                        for (var section in parsed) {
                                            var fields = parsed[section];
                                            for (var key in fields) {
                                                var val = fields[key];
                                        
                                                // Insert into DataTable
                                         
                                                Things["ArorianStatusTable"].AddDataTableEntry({
                                                    values: {
                                                        Section: section,
                                                        Field: key,
                                                        Value: (val !== null && val !== undefined) ? val.toString() : ""
                                                    }
                                                });
                                            }
                                        }
